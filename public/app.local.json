{
    "startup_app": {
        "name": "Assistant",
        "url": "http://localhost:5555/index.html",
        "uuid": "Assistant",
        "autoShow": true,
        "maximizable": false,
        "showTaskbarIcon": false,
        "saveWindowState": false,
        "frame": false,
        "defaultHeight": 230,
        "defaultWidth": 230,
        "alphaMask": {
            "red": 200,
            "green": 200,
            "blue": 200
        }
    },
    "runtime": {
        "arguments": "--security-realm=clippy --enable-mesh --disable-gpu --disable-legacy-window",
        "version": "9.*"
    }
}